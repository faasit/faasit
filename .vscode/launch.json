{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python 调试程序: 模块",
      "type": "debugpy",
      "request": "launch",
      "module": "serverless_framework.distributed_controller",
      "args": [
        "--repeat",
        "1",
        "--launch",
        "tradition",
        "--transmode",
        "auto",
        "--redis_yaml",
        "distributed-redis.yaml",
        "--redis_secret_yaml",
        "distributed-redis-secret.yaml",
        "--ditto_placement",
        "--redis_num",
        "1",
        "--multi_config_path",
        "workflow_gpu"
      ],
      "cwd": "${fileDirname}"
    },
    {
      "name": "Python 调试程序: 当前文件",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${fileDirname}",
    },
    {
      "name": "Faasit deploy",
      "program": "${workspaceFolder}/packages/faasit/bin/faasit.cjs",
      "request": "launch",
      "cwd": "${workspaceFolder}/packages/faasit-examples/examples/dag-wordcount",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "args": [
        "deploy"
      ]
    },
    {
      "name": "Faasit invoke",
      "program": "${workspaceFolder}/packages/faasit/bin/faasit.cjs",
      "request": "launch",
      "cwd": "${workspaceFolder}/packages/faasit-examples/examples/demo1/aliyun",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "args": [
        "invoke"
      ]
    },
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "${workspaceFolder}/demo",
        "--extensionDevelopmentPath=${workspaceFolder}/packages/vscode-faasit",
        "--disable-extensions"
      ],
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ],
      "preLaunchTask": "vscode-faasit:build"
    },
    {
      "name": "Run Extension Demo 1",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "${workspaceFolder}/packages/faasit-examples/examples/nodejs-hello",
        "--extensionDevelopmentPath=${workspaceFolder}/packages/vscode-faasit",
        "--disable-extensions",
      ],
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ],
      "preLaunchTask": "vscode-faasit:build"
    },
    {
      "name": "Run Web Extension in VS Code",
      "type": "extensionHost",
      "debugWebWorkerHost": true,
      "request": "launch",
      "args": [
        "--folder-uri=memfs:/sample-folder",
        "--extensionDevelopmentPath=${workspaceFolder}/packages/vscode-faasit",
        "--extensionDevelopmentPath=${workspaceFolder}/packages/vscode-faasit-cloud",
        "--extensionDevelopmentKind=web",
        "--enable-proposed-api=brody715.vscode-faasit-cloud",
        "--disable-extensions",
      ],
      "outFiles": [
        "${workspaceFolder}/packages/vscode-faasit/out/**/*.js"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "preLaunchTask": "vscode-extension:build"
    }
  ]
}