@function func {
    runtime = "python"
    codeDir = "./code"
    handler = "index.func"
}

@application helloapp {
    functions = [func]
    defaultProvider = kn
    providers = [kn,k8s,ali]
}

@provider kn {
    kind = 'knative'
    registry = "192.168.0.100:5000"
    redis_data = "./data"
}

@provider k8s {
    kind = 'k8s'
    registry = "192.168.0.100:5000"
    redis_data = "./data"
}

@provider ali {
    kind = "aliyun"
    oss = {
        bucket = "test"
        region = "cn-handzhou"
    }
}